<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_at_sign_in_the_suffix</fullName>
    <active>true</active>
    <errorConditionFormula>CONTAINS(Suffix__c, &quot;@&quot;)</errorConditionFormula>
    <errorDisplayField>Suffix__c</errorDisplayField>
    <errorMessage>The suffix should be the stuff AFTER the @ sign, not including the @ sign itself.</errorMessage>
</ValidationRule>
